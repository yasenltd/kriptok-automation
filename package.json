{
  "name": "kriptok-wallet",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "e2e:build": "detox build -c ios.sim.debug && npx expo start",
    "e2e:setup": "detox test -c ios.sim.debug e2e/tests/setup.test.js",
    "e2e:importFlow:tests": "npm run e2e:setup && detox test -c ios.sim.debug --reuse e2e/tests/importWallet.test.js",
    "e2e:createFlow:tests": "npm run e2e:setup && detox test -c ios.sim.debug --reuse e2e/tests/createWallet.test.js && detox test -c ios.sim.debug --reuse e2e/tests/seedBackup.test.js",
    "e2e:setup:ci": "npx -y detox@20.40.2 test -c ios.sim.debug e2e/tests/setup.test.js",
    "e2e:createFlow:tests:ci": "bash -eo pipefail -c 'npx expo start --dev-client --clear --port 8081 & METRO_PID=$!; npx -y wait-on tcp:127.0.0.1:8081; npx -y detox@20.40.2 test -c ios.sim.debug --headless --record-logs all --take-screenshots all e2e/tests/setup.test.js; npx -y detox@20.40.2 test -c ios.sim.debug --headless --record-logs all --take-screenshots all --reuse e2e/tests/createWallet.test.js; npx -y detox@20.40.2 test -c ios.sim.debug --headless --record-logs all --take-screenshots all --reuse e2e/tests/seedBackup.test.js; EXIT=$?; kill $METRO_PID 2>/dev/null || true; exit $EXIT'",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
